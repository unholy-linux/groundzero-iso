	gap_border     			= { 1, 1, 0, 0 }
	gap_side			= TOP
    }
    image 
    {
        function			= BOX_GAP
	file				= "images/tabs/notebook.svg"
	border				= { 4, 4, 4, 4 }
	stretch				= TRUE
	gap_file			= "images/tabs/notebook-gap-horiz.svg"
	gap_border			= { 1, 1, 0, 0 }
	gap_side			= BOTTOM
    }
    image 
    {
        function			= BOX_GAP
