style "panel-buttons"
{
	xthickness            			= 4
	ythickness            			= 4
	GtkWidget::focus_padding 		= 2
	
	fg[NORMAL]			= "#cdcdcf"
	fg[PRELIGHT]		= @selected_fg_color
	fg[SELECTED]		= @selected_fg_color
	fg[INSENSITIVE]		= "#5a5a5d"
	fg[ACTIVE]			= "#fff"

	engine "pixmap"
	 { 
	image
	{
		function        		= BOX
		recolorable     		= TRUE
		state					= NORMAL
		file            		= "images/buttons/panel/panel-normal.svg"
		border          		= { 4, 4, 5,5 }
		stretch         		= TRUE
	}	
	image
	{
		function        		= BOX
		shadow					= OUT
		recolorable     		= TRUE
		state					= PRELIGHT
		file            		= "images/buttons/panel/panel-hover.svg"
		border          		= { 4, 4, 5,5 }
		stretch         		= TRUE
	}	
    image
	{
		function        		= BOX
		recolorable     		= TRUE
		shadow					= IN
		state					= PRELIGHT
		file            		= "images/buttons/panel/panel-active.svg"
		border          		= { 4, 4, 5,5 }
		stretch         		= TRUE
	}	
	image
	{
		function        		= BOX
		recolorable     		= TRUE
		state					= ACTIVE
		file            		= "images/buttons/panel/panel-active.svg"
		border          		= { 4, 4, 5,5 }
		stretch         		= TRUE
	}  
	image
	{
		function        		= BOX
		recolorable     		= TRUE
		state					= INSENSITIVE
		file            		= "images/buttons/panel/panel-normal.svg"
		border          		= { 4, 4, 5,5 }
		stretch         		= TRUE
	} 
	image
	{
		function        		= BOX
		recolorable     		= TRUE
		state					= SELECTED
		file            		= "images/buttons/panel/panel-active.svg"
		border          		= { 4, 4, 5,5 }
		stretch         		= TRUE
	}  
 }
}

style "panel-toggle-buttons" = "theme-panel"
{
	GtkWidget::focus_padding = 0
	
	xthickness            			= 4
	ythickness            			= 1

	bg[NORMAL]	= shade (1.1, "#0a0a0a")
	bg[ACTIVE]	= shade (0.9, "#0a0a0a")
	bg[PRELIGHT]	= shade (1.3, "#0a0a0a")
	bg[SELECTED]	= shade (0.78, "#0a0a0a")

	engine "pixmap" 
   {
	image
	{
		function        		= BOX
		recolorable     		= TRUE
		state					= NORMAL
		file            		= "images/buttons/menubar-normal.svg"
		border          		= { 0,0,0,0 }
		stretch         		= TRUE
	}
	image
	{
		function				= BOX
		recolorable				= TRUE
		shadow					= OUT
		state					= PRELIGHT
		file					= "images/buttons/menubar-normal.svg"
		border					= { 0,0,0,0}
		stretch					= TRUE
	}
	image
	{
		function        		= BOX
		recolorable     		= TRUE
		shadow					= IN
		state					= PRELIGHT
		file            		= "images/buttons/menubar-pressed.svg"
		border          		= { 5, 5 , 4 , 3}
		stretch         		= TRUE
	}
	image
	{
		function        		= BOX
		recolorable     		= TRUE
		state					= ACTIVE
		file					= "images/buttons/menubar-pressed.svg"
		border					= { 5, 5 , 4 , 3}
		stretch         		= TRUE
	} 
	image
	{
		function        		= BOX
		recolorable     		= TRUE
		state					= INSENSITIVE
		file					= "images/buttons/menubar-normal.svg"
		border					= { 0,0,0,0 }
		stretch         		= TRUE
	}
	image
	{
		function        		= BOX
		recolorable     		= TRUE
		state					= SELECTED
		file					= "images/buttons/menubar-pressed.svg"
		border					= { 5, 5 , 4 , 3}
		stretch         		= TRUE
	}  
  }
}


