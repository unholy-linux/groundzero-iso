	text[ACTIVE]			= "#bbbbbd"

	base[NORMAL]			= @base_color #This is the Text-Input-Area 
										#color in Chromium and Eclipse 
										#window and scrolled window.
	base[PRELIGHT]			= shade (1.2, @base_color)
	base[SELECTED]			= @selected_bg_color
	base[INSENSITIVE]		= "#5a5a5f"
	base[ACTIVE]			= shade (0.9, @base_color)
	
