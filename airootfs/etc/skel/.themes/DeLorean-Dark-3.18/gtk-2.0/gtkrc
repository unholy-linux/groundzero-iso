      orientation		= HORIZONTAL
    }
    image 
    {
      function			= BOX
      detail			= "trough"
      file			= "images/range/trough-border-v.svg"
      border			= { 8,8,6,7 }
      stretch			= TRUE
      orientation		= VERTICAL
    }

    # Horizontal#######################################################

    image 
    {
      function				= SLIDER
      state           			= NORMAL
      file				= "images/range/null.png"
      border				= { 0, 0, 0, 0 }
      stretch				= TRUE
      overlay_file			= "images/range/scale-slider.svg"
      overlay_stretch			= FALSE
      orientation			= HORIZONTAL
    }
    image 
    {
      function				= SLIDER
      state           			= PRELIGHT
      file				= "images/range/null.png"
      border				= { 0, 0, 0, 0 }
      stretch				= TRUE
      overlay_file			= "images/range/scale-slider-hover.svg"
      overlay_stretch			= FALSE
      orientation			= HORIZONTAL
    }
    image 
    {
      function				= SLIDER
      state           			= INSENSITIVE
      file				= "images/range/null.png"
      border				= { 0, 0, 0, 0 }
      stretch				= TRUE
      overlay_file			= "images/range/scale-slider-insensitive.svg"
      overlay_stretch			= FALSE
      orientation			= HORIZONTAL
    }

    # Vertical#########################################################

    image 
    {
      function				= SLIDER
      state           			= NORMAL
      file				= "images/range/null.png"
      border				= { 0, 0, 0, 0 }
      stretch				= TRUE
      overlay_file			= "images/range/scale-slider.svg"
      overlay_stretch			= FALSE
      orientation			= VERTICAL
    }
    image 
    {
      function				= SLIDER
      state           			= PRELIGHT
      file				= "images/range/null.png"
      border				= { 0, 0, 0, 0 }
      stretch				= TRUE
      overlay_file			= "images/range/scale-slider-hover.svg"
      overlay_stretch			= FALSE
      orientation			= VERTICAL
    }
    image 
    {
      function				= SLIDER
      state           			= INSENSITIVE
      file				= "images/range/null.png"
      border				= { 0, 0, 0, 0 }
      stretch				= TRUE
      overlay_file			= "images/range/scale-slider-insensitive.svg"
      overlay_stretch			= FALSE
      orientation			= VERTICAL
    }

    # Function below removes ugly boxes################################
    image 
    {
       	function		= BOX
	file        		= "images/range/null.png"
	border      		= { 3, 3, 3, 3 }
	stretch         	= TRUE
    }

  }
