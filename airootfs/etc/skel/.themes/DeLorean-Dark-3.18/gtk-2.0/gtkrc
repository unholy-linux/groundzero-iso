#Entry Firefox#########################################################
style "ff_entry"  
{
	xthickness		= 3
	ythickness   	= 4
	
  base[NORMAL] 		= "#dbdbdb" #Spotify and other non gtk apps use 
								#this as the entry text-area-input#
  base[INSENSITIVE] = "#cdcdcd"        
  text[SELECTED]  	= "#fff"
  text[NORMAL] 		= shade(0.5, @base_color)
  text[INSENSITIVE] = shade(2.5, mix(0.03, @selected_bg_color,  
							shade(1.1, @base_color)))
  
	engine "pixmap"
	{
	image
	{
		function		= SHADOW
		detail			= "entry"
                state                   = NORMAL
		shadow			= IN
		file			= "images/entry/ff-entry.svg"
