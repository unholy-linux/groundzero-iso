	gap_side			= LEFT
    }
    image 
    {
        function			= BOX_GAP
	file				= "images/tabs/notebook.svg" 
	border				= { 4, 4, 4, 4 }
	stretch				= TRUE
	gap_file			= "images/tabs/notebook-gap-vert.svg"
	gap_border			= { 0, 0, 1, 1 }
	gap_side			= RIGHT
    }

